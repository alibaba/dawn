{
  "name": "dawn",
  "version": "0.17.0",
  "description": "dawn cli",
  "main": "./lib/index.js",
  "bin": {
    "dn": "./bin/cli.js",
    "dawn": "./bin/cli.js"
  },
  "scripts": {
    "dnconfig": "node ./bin/cli.js config registry https://registry.npmjs.com/",
    "dntest": "node ./bin/cli.js test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dnbuild": "node ./bin/cli.js build",
    "test": "npm run dnconfig && npm run dntest",
    "gitrebase": "git pull --rebase `echo $npm_package_repository_url | cut -d \\+ -f 2` master"
  },
  "pre-push": [
    "gitrebase",
    "test",
    "dnbuild"
  ],
  "keywords": [
    "dawn",
    "cli",
    "dn",
    "generate",
    "build"
  ],
  "engines": {
    "node": ">=7.6.0"
  },
  "configs": {
    "server": "https://alibaba.github.io/dawn",
    "registry": "https://registry.npm.taobao.org",
    "cacheTTL": 3600000,
    "middlewarePrefix": "dn-middleware",
    "templatePrefix": "dn-template"
  },
  "author": {
    "name": "Houfeng",
    "email": "admin@xhou.net"
  },
  "homepage": "http://alibaba.github.io/dawn",
  "bugs": {
    "url": "https://github.com/alibaba/dawn/issues",
    "email": "admin@xhou.net"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba/dawn.git"
  },
  "dependencies": {
    "buffer-to-stream": "^1.0.0",
    "bufferhelper": "^0.2.1",
    "cmdline": "^2.0.4",
    "confman": "^0.2.8",
    "console3": "^1.0.6",
    "copy-dir": "^0.3.0",
    "debug": "^3.1.0",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "del": "^3.0.0",
    "globby": "^7.1.1",
    "inquirer": "^5.0.1",
    "js-yaml": "^3.10.0",
    "leftpad": "^0.0.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.3",
    "npm": "^6.4.1",
    "ntils": "^3.1.11",
    "oneport": "^1.0.2",
    "react-dev-utils": "^5.0.1",
    "require-dir": "^0.3.2",
    "rightpad": "^1.0.1",
    "semver": "^5.4.1",
    "shify": "^3.0.5",
    "stp": "0.0.4",
    "stream-to-buffer": "^0.1.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.0",
    "dn-middleware-doc": "^2.1.0",
    "dn-middleware-file-header": "^1.0.3",
    "dn-middleware-lint": "^2.1.0",
    "dn-middleware-unit": "^0.1.18",
    "eslint-config-dawn": "^1.0.7",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-react": "^7.11.1",
    "nyc": "^11.4.1"
  }
}
